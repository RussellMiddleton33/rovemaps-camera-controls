{
  "name": "three-rovemaps-camera-controls",
  "version": "0.0.0-dev",
  "description": "RoveMaps MAP Camera Controls for Three.js",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "peerDependencies": {
    "three": ">=0.153 <0.170"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "jsdom": "^27.0.0",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.5",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "vitepress": "^1.3.0",
    "three": "^0.158.0",
    "vitest": "^2.0.3"
  }
}
